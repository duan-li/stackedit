{
  "hosting": {
    "public": "public",
    "ignore": [
      "**/.*",
      "**/node_modules/**",
      "firebase.json",
      "**/chrome-app/**",
      "**/build/**",
      "**/test/**",
      "**/src/**"
    ],
    "redirects": [ {
      "source" : "/oauth2/callback",
      "destination" : "/static/oauth2/callback.html",
      "type" : 301
      }, {
      "source" : "/app",
      "destination" : "/index.html",
      "type" : 301
      } ]
  }
}